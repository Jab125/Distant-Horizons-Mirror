{
  "schemaVersion": 1,
  "id": "lod",
  "version": "${version}",

  "name": "${mod_name}",
  "description": "${description}",
  "authors": ["${authors}"],

  "contact": {
    "homepage": "${homepage}",
    "sources": "${source}",
    "issues": "${issues}"
  },

  "license": "CC0-1.0",
  "icon": "icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "com.seibel.lod.fabric.Main"
    ],
    "modmenu": [
      "com.seibel.lod.fabric.wrappers.config.ModMenuIntegration"
    ]
  },

  "mixins": [
    "fabric.lod.mixins.json"
  ],

  "accessWidener" : "lod.accesswidener",
  "depends": {
    "fabricloader": "*",
    "fabric": "*",
    "minecraft": "1.18.2",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  },

  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/xAB8G4cENx"
      }
    }
  }
}
